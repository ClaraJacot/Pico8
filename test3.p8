--messages

function init_msg()
message={}
create_msg("but du jeu",
"rendez vous au safe place\nderriere les portes dorees")
end

function create_msg(name,...)
msg_title=name
message={...}
end

function update_msg()
	if (btnp(❎)) then
		deli(message,1)
	end	
end

function draw_msg()
	if message[1] then
		local y=100
			if p.y%16>=9 then
				y=10
			end	
		rectfill(6,y,
		6+#msg_title*4,y+6,2)	
		print (msg_title,7,y+1,7)
		rectfill(2,y+9,125,y+21,13)
		print (message[1],3,y+10,7)
	end	
end
